
<div class="list_skatelogs">

  <% skatelogs.each do |skatelog| %>

    <div class="skatelog">
      <div class="title">
        <%= skatelog.title %>
      </div>

      <div class ="profpic">
        <img src="#profpicUrl">
      </div>

      <div class="username">
        <a href="/user/:id"><p>@<%= skatelog.user.username %></p></a>
      </div>

      <div class="description">
        <%= skatelog.description %>
      </div>
    </div>
  <% end %>
</div>
